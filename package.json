{
  "name": "clients.harbor-centre",
  "version": "0.1.0",
  "description": "Website for Harbor Centre client",
  "main": "index.js",
  "scripts": {
    "tf-next": "tf-next build ./src",
    "dev": "next ./src",
    "beta:init": "cd deployment/beta && terraform init",
    "beta:deploy": "cd deployment/beta && terraform apply -auto-approve -var-file=./beta.tfvars",
    "beta:destroy": "cd deployment/beta && terraform destroy -auto-approve -var-file=./beta.tfvars",
    "beta:plan": "cd deployment/beta && terraform plan -var-file=./beta.tfvars",
    "prod:init": "cd deployment/prod && terraform init",
    "prod:deploy": "cd deployment/prod && terraform apply -auto-approve -var-file=./prod.tfvars",
    "prod:destroy": "cd deployment/prod && terraform destroy -auto-approve -var-file=./prod.tfvars",
    "prod:plan": "cd deployment/prod && terraform plan -var-file=./prod.tfvars",
    "lint": "eslint \"./**/*.{js,jsx}\"",
    "gen:comp": "node ./node_modules/@ebsolutions/generate-react-templates --type=COMP",
    "gen:cont": "node ./node_modules/@ebsolutions/generate-react-templates --type=CONT",
    "gen:page": "node ./node_modules/@ebsolutions/generate-react-templates --type=PAGE"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronsisler/clients.harbor-centre.git"
  },
  "author": "Aaron Sisler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aaronsisler/clients.harbor-centre/issues"
  },
  "homepage": "https://github.com/aaronsisler/clients.harbor-centre#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@hookform/resolvers": "^2.8.3",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "email-validator": "^2.0.4",
    "next": "^12.0.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.20.2",
    "sass": "^1.43.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@ebsolutions/generate-react-templates": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.4",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "stylelint": "^14.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "tf-next": "^0.10.2"
  }
}
